#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/smahmed/web-development/flojoy/cloud/zapier/node_modules/.pnpm/npm-packlist@2.2.2/node_modules/npm-packlist/bin/node_modules:/Users/smahmed/web-development/flojoy/cloud/zapier/node_modules/.pnpm/npm-packlist@2.2.2/node_modules/npm-packlist/node_modules:/Users/smahmed/web-development/flojoy/cloud/zapier/node_modules/.pnpm/npm-packlist@2.2.2/node_modules:/Users/smahmed/web-development/flojoy/cloud/zapier/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/smahmed/web-development/flojoy/cloud/zapier/node_modules/.pnpm/npm-packlist@2.2.2/node_modules/npm-packlist/bin/node_modules:/Users/smahmed/web-development/flojoy/cloud/zapier/node_modules/.pnpm/npm-packlist@2.2.2/node_modules/npm-packlist/node_modules:/Users/smahmed/web-development/flojoy/cloud/zapier/node_modules/.pnpm/npm-packlist@2.2.2/node_modules:/Users/smahmed/web-development/flojoy/cloud/zapier/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/index.js" "$@"
else
  exec node  "$basedir/../../bin/index.js" "$@"
fi
