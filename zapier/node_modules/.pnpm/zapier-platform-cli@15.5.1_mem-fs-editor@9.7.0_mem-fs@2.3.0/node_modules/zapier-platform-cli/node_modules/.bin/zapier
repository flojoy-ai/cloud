#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/smahmed/web-development/flojoy/cloud/zapier/node_modules/.pnpm/zapier-platform-cli@15.5.1_mem-fs-editor@9.7.0_mem-fs@2.3.0/node_modules/zapier-platform-cli/src/bin/node_modules:/Users/smahmed/web-development/flojoy/cloud/zapier/node_modules/.pnpm/zapier-platform-cli@15.5.1_mem-fs-editor@9.7.0_mem-fs@2.3.0/node_modules/zapier-platform-cli/src/node_modules:/Users/smahmed/web-development/flojoy/cloud/zapier/node_modules/.pnpm/zapier-platform-cli@15.5.1_mem-fs-editor@9.7.0_mem-fs@2.3.0/node_modules/zapier-platform-cli/node_modules:/Users/smahmed/web-development/flojoy/cloud/zapier/node_modules/.pnpm/zapier-platform-cli@15.5.1_mem-fs-editor@9.7.0_mem-fs@2.3.0/node_modules:/Users/smahmed/web-development/flojoy/cloud/zapier/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/smahmed/web-development/flojoy/cloud/zapier/node_modules/.pnpm/zapier-platform-cli@15.5.1_mem-fs-editor@9.7.0_mem-fs@2.3.0/node_modules/zapier-platform-cli/src/bin/node_modules:/Users/smahmed/web-development/flojoy/cloud/zapier/node_modules/.pnpm/zapier-platform-cli@15.5.1_mem-fs-editor@9.7.0_mem-fs@2.3.0/node_modules/zapier-platform-cli/src/node_modules:/Users/smahmed/web-development/flojoy/cloud/zapier/node_modules/.pnpm/zapier-platform-cli@15.5.1_mem-fs-editor@9.7.0_mem-fs@2.3.0/node_modules/zapier-platform-cli/node_modules:/Users/smahmed/web-development/flojoy/cloud/zapier/node_modules/.pnpm/zapier-platform-cli@15.5.1_mem-fs-editor@9.7.0_mem-fs@2.3.0/node_modules:/Users/smahmed/web-development/flojoy/cloud/zapier/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../src/bin/run" "$@"
else
  exec node  "$basedir/../../src/bin/run" "$@"
fi
