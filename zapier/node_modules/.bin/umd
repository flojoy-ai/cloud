#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/smahmed/web-development/flojoy/cloud/zapier/node_modules/.pnpm/umd@3.0.3/node_modules/umd/bin/node_modules:/Users/smahmed/web-development/flojoy/cloud/zapier/node_modules/.pnpm/umd@3.0.3/node_modules/umd/node_modules:/Users/smahmed/web-development/flojoy/cloud/zapier/node_modules/.pnpm/umd@3.0.3/node_modules:/Users/smahmed/web-development/flojoy/cloud/zapier/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/smahmed/web-development/flojoy/cloud/zapier/node_modules/.pnpm/umd@3.0.3/node_modules/umd/bin/node_modules:/Users/smahmed/web-development/flojoy/cloud/zapier/node_modules/.pnpm/umd@3.0.3/node_modules/umd/node_modules:/Users/smahmed/web-development/flojoy/cloud/zapier/node_modules/.pnpm/umd@3.0.3/node_modules:/Users/smahmed/web-development/flojoy/cloud/zapier/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../umd/bin/cli.js" "$@"
else
  exec node  "$basedir/../umd/bin/cli.js" "$@"
fi
