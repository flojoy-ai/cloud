{
  "name": "@cloud/server",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "build": "bun build src/index.ts --outdir ./dist --target bun",
    "lint": "eslint .",
    "start": "bun run dist/index.js",
    "test": "bun test",
    "dev": "bun run --watch src/index.ts",
    "kanel": "NODE_TLS_REJECT_UNAUTHORIZED=0 bunx kanel",
    "migrate:up": "bun run ./scripts/migrate.ts --up",
    "migrate:down": "bun run ./scripts/migrate.ts --down",
    "migrate:to-latest": "bun run ./scripts/migrate.ts --to-latest",
    "migrate:to": "bun run ./scripts/migrate.ts --to"
  },
  "dependencies": {
    "@bogeychan/elysia-logger": "^0.0.20",
    "@cloud/eslint-config": "workspace:*",
    "@cloud/tsconfig": "workspace:*",
    "@cloud/shared": "workspace:*",
    "@elysiajs/cors": "^1.0.2",
    "@elysiajs/swagger": "^1.0.3",
    "@lucia-auth/adapter-postgresql": "^3.1.2",
    "@paralleldrive/cuid2": "^2.2.2",
    "@t3-oss/env-core": "^0.9.2",
    "arctic": "^1.5.0",
    "elysia": "^1.0.10",
    "kanel": "^3.8.8",
    "kanel-kysely": "^0.3.3",
    "kysely": "^0.27.3",
    "lucia": "^3.1.1",
    "neverthrow": "^6.1.0",
    "oslo": "^1.2.0",
    "superjson": "^2.2.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@total-typescript/ts-reset": "^0.5.1",
    "bun-types": "^1.0.36"
  },
  "module": "src/index.ts",
  "main": "src/index.ts"
}
