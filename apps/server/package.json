{
  "name": "@cloud/server",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "build": "bun build src/index.ts --outdir ./dist --target bun",
    "lint": "eslint .",
    "start": "bun run dist/index.js",
    "test": "bun test",
    "dev": "bun run --watch src/index.ts",
    "kanel": "bunx kanel",
    "migrate:up": "dotenv -c -- tsx ./scripts/migrate.ts --up",
    "migrate:down": "dotenv -c -- tsx ./scripts/migrate.ts --down",
    "migrate:to-latest": "dotenv -c -- tsx ./scripts/migrate.ts --to-latest",
    "migrate:to": "dotenv -c -- tsx ./scripts/migrate.ts --to"
  },
  "dependencies": {
    "@cloud/eslint-config": "workspace:*",
    "@cloud/tsconfig": "workspace:*",
    "@bogeychan/elysia-logger": "^0.0.19",
    "@elysiajs/cors": "^1.0.2",
    "@elysiajs/swagger": "^0.8.5",
    "@lucia-auth/adapter-postgresql": "^3.1.1",
    "@paralleldrive/cuid2": "^2.2.2",
    "@t3-oss/env-core": "^0.9.2",
    "arctic": "^1.2.1",
    "elysia": "^1.0.7",
    "kysely": "^0.27.3",
    "lucia": "^3.1.1",
    "neverthrow": "^6.1.0",
    "oslo": "^1.1.3",
    "superjson": "^2.2.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@total-typescript/ts-reset": "^0.5.1",
    "bun-types": "latest",
    "dotenv-cli": "^7.4.1",
    "tsx": "^4.7.1"
  },
  "module": "src/index.ts",
  "main": "src/index.ts"
}
