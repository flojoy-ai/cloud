{
  "name": "@cloud/server",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "build": "bun build src/index.ts",
    "lint": "eslint .",
    "start": "NODE_ENV=production bun src/index.ts",
    "test": "bun test",
    "dev": "bun run --watch src/index.ts",
    "kanel": "bunx kanel",
    "migrate:up": "dotenv -c -- tsx ./scripts/migrate.ts --up",
    "migrate:down": "dotenv -c -- tsx ./scripts/migrate.ts --down",
    "migrate:to-latest": "dotenv -c -- tsx ./scripts/migrate.ts --to-latest",
    "migrate:to": "dotenv -c -- tsx ./scripts/migrate.ts --to"
  },
  "dependencies": {
    "@bogeychan/elysia-logger": "^0.0.19",
    "@cloud/eslint-config": "workspace:*",
    "@elysiajs/cors": "^1.0.2",
    "@elysiajs/swagger": "^0.8.5",
    "@sinclair/typebox": "^0.32.18",
    "@t3-oss/env-core": "^0.9.2",
    "elysia": "^1.0.7",
    "lucia": "^3.1.1",
    "neverthrow": "^6.1.0",
    "oslo": "^1.1.3",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@total-typescript/ts-reset": "^0.5.1",
    "bun-types": "latest",
    "dotenv-cli": "^7.4.1",
    "tsx": "^4.7.1"
  },
  "module": "src/index.js"
}
